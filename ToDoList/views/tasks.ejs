<!DOCTYPE html>
<html lang="en">
<head>
    <title>To-Do List</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <h1>All Tasks</h1>
    <a href="/add-task">Add New Task</a>
    <ul>
        <% tasks.forEach(task => { %>
            <li><a href="/task?id=<%= task.id %>"><%= task.title %></a></li>
        <% }) %>
    </ul>
</body>
</html>
